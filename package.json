{
  "name": "andt-fuski",
  "version": "1.1.2",
  "description": "A list of wrapped react components based on ant design.",
  "author": "Mohamed Ben Rhaiem",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mohamedRhaiem/andt-fuski/issues"
  },
  "homepage": "https://github.com/mohamedRhaiem/andt-fuski#readme",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mohamedRhaiem/andt-fuski.git"
  },
  "keywords": [
    "andt",
    "reactjs",
    "storybook"
  ],
  "scripts": {
    "storybook": "start-storybook -p 6006 --no-manager-cache",
    "build-storybook": "build-storybook",
    "build": "rimraf dist && NODE_ENV=production  BABEL_ENV=production rollup -c",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet",
    "lint:fix": "eslint --fix --cache --quiet --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:style": "stylelint --fix \"src/**/*.less\" \"assets/**/*.less\" --syntax less",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@ant-design/icons": "^4.6.2",
    "@babel/core": "^7.13.10",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@storybook/addon-actions": "^6.3.2",
    "@storybook/addon-essentials": "^6.3.2",
    "@storybook/addon-links": "^6.3.2",
    "@storybook/react": "^6.3.2",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/jest": "^26.0.21",
    "@types/lodash": "^4.14.169",
    "@types/node": "^14.14.35",
    "@types/testing-library__react": "^10.2.0",
    "@typescript-eslint/parser": "^4.23.0",
    "antd": "^4.9.1",
    "babel-loader": "^8.2.2",
    "babel-preset-react-app": "^10.0.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-formatter-pretty": "^4.0.0",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "fork-ts-checker-webpack-plugin": "^4.1.6",
    "http-proxy-middleware": "^2.0.1",
    "jest": "^26.6.3",
    "less": "^2.7.3",
    "less-loader": "7.3.0",
    "lint-staged": "^10.5.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-draggable": "^4.4.3",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "stylelint": "^13.12.0",
    "stylelint-config-recommended": "^4.0.0",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3",
    "uuidv4": "^6.2.8"
  },
  "peerDependencies": {
    "antd": "^4.9.1",
    "lodash": "^4.17.21",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "dependencies": {
    "@rollup/plugin-node-resolve": "^13.0.0",
    "rollup": "^2.50.5",
    "yarn": "^1.22.10"
  },
  "babel": {
    "presets": [
      [
        "react-app",
        {
          "typescript": true,
          "flow": false
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-proposal-optional-chaining",
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-proposal-class-properties",
      "transform-react-remove-prop-types"
    ],
    "exclude": "node_modules"
  }
}
